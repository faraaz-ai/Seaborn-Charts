    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "data=sns.load_dataset(\"tips\")\n",
    "print(data)\n",
    "\n",
    "a=sns.FacetGrid(data,col=\"smoker\",height=5,hue=\"sex\")#we can give any heightand facetgrid is used for multiple plot\n",
    "\n",
    "a.map(sns.barplot,\"day\",\"tip\")\n",
    "plt.show()"
